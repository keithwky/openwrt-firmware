# CONFIG_PACKAGE_6to4 is not set
# CONFIG_PACKAGE_6rd is not set

# WLAN/WPS support
# CONFIG_PACKAGE_hostapd-utils is not set
CONFIG_WPA_MSG_MIN_PRIORITY=2
## CONFIG_PACKAGE_batctl=y

# SSL Certificates
# CONFIG_PACKAGE_ca-certificates is not set
CONFIG_PACKAGE_ca-bundle=y

# Luci (SSL from OpenSSL)
# CONFIG_PACKAGE_luci-app-ddns is not set
# CONFIG_PACKAGE_luci-app-sqm is not set
# CONFIG_PACKAGE_luci-app-uhttpd is not set
# CONFIG_PACKAGE_luci-app-upnp is not set
CONFIG_LUCI_JSMIN=y
CONFIG_LUCI_SRCDIET=y
CONFIG_PACKAGE_uhttpd-mod-lua=y
CONFIG_PACKAGE_uhttpd-mod-ubus=y

# Luci statistics
# CONFIG_PACKAGE_collectd-mod-dhcpleases is not set
# CONFIG_PACKAGE_collectd-mod-sqm is not set
# CONFIG_PACKAGE_collectd-mod-uptime is not set

# Build openwrt-2020 theme, default is still bootstrap
# CONFIG_PACKAGE_luci-theme-bootstrap is not set
# CONFIG_PACKAGE_luci-theme-material is not set
# CONFIG_PACKAGE_luci-theme-openwrt is not set
CONFIG_PACKAGE_luci-theme-openwrt-2020=y

# kernel support for tunnels, VPNs
# CONFIG_PACKAGE_kmod-tun is not set

